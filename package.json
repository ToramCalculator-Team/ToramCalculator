{
  "name": "toram-calculator",
  "type": "module",
  "scripts": {
    "dev": "vinxi dev",
    "dev:init": "pnpm dev:init:inject && pnpm dev:init:generate",
    "dev:init:inject": "tsx db/generator/injectEnums.ts",
    "dev:init:generate": "prisma generate --schema=db/generated/schema.prisma",
    "dev:setup": "pnpm backend:down && pnpm backend:up -d && pnpm db:restore",
    "backend:up": "docker compose --env-file .env -f ./backend/docker-compose.yaml up",
    "backend:stop": "docker compose --env-file .env -f ./backend/docker-compose.yaml stop",
    "backend:down": "docker compose --env-file .env -f ./backend/docker-compose.yaml down --volumes",
    "db:studio": "prisma studio --schema=db/generated/schema.prisma",
    "db:backup": "tsx db/scripts/backup.ts",
    "db:restore": "tsx db/scripts/restore.ts",
    "build": "node src/worker/sw/build.mjs && vinxi build",
    "start": "vinxi start",
    "clear-build": "rm -rf .vinxi/ .output bundle.tar.gz public/service.worker.js public/chunk-manifest.json public/service.worker.js db/generated",
    "package": "rm -rf .vinxi/ .output bundle.tar.gz && pnpm build && tar -czf bundle.tar.gz .vinxi/ .output/",
    "prepare": "husky"
  },
  "devDependencies": {
    "@babylonjs/inspector": "^7.32.5",
    "@prisma/generator-helper": "6.18.0",
    "@types/bcrypt": "^5.0.2",
    "@types/js-cookie": "^3.0.6",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.7.9",
    "@types/pg": "^8.11.11",
    "esbuild": "^0.25.6",
    "eslint": "^9.9.0",
    "husky": "^9.1.4",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "prisma": "6.18.0",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.0",
    "vite-plugin-top-level-await": "^1.5.0"
  },
  "dependencies": {
    "@auth/core": "^0.38.0",
    "@auth/pg-adapter": "^1.8.0",
    "@auth/solid-start": "^0.16.0",
    "@babylonjs/core": "^7.32.5",
    "@babylonjs/loaders": "^7.32.5",
    "@babylonjs/materials": "^7.32.5",
    "@editorjs/editorjs": "^2.30.4",
    "@editorjs/header": "^2.8.8",
    "@editorjs/link": "^2.6.2",
    "@editorjs/list": "^2.0.6",
    "@editorjs/quote": "^2.7.6",
    "@electric-sql/pglite": "latest",
    "@electric-sql/pglite-sync": "latest",
    "@mrleebo/prisma-ast": "^0.13.1",
    "@paralleldrive/cuid2": "^2.2.2",
    "@prisma/client": "6.18.0",
    "@query-builder/solid-query-builder": "^1.0.1",
    "@solid-primitives/scheduled": "^1.5.0",
    "@solidjs/meta": "^0.28.0",
    "@solidjs/router": "^0.15.3",
    "@solidjs/start": "latest",
    "@tailwindcss/typography": "^0.5.14",
    "@tailwindcss/vite": "^4.0.9",
    "@tanstack/solid-form": "latest",
    "@tanstack/solid-table": "latest",
    "@tanstack/solid-virtual": "^3.9.0",
    "@xstate/solid": "^2.0.0",
    "acorn": "^8.15.0",
    "bcrypt": "^5.1.1",
    "blockly": "^11.2.1",
    "comlink": "^4.4.1",
    "dotenv": "^16.4.7",
    "dotenv-expand": "^12.0.1",
    "hotkeys-js": "^3.13.7",
    "jose": "^5.9.6",
    "js-cookie": "^3.0.5",
    "kysely": "^0.27.4",
    "lodash-es": "^4.17.21",
    "mathjs": "^13.0.3",
    "nodemailer": "^6.10.0",
    "overlayscrollbars": "^2.12.0",
    "overlayscrollbars-solid": "^0.5.6",
    "pg": "^8.14.1",
    "sequential-workflow-designer": "^0.27.0",
    "solid-js": "^1.9.9",
    "solid-motionone": "^1.0.4",
    "tailwindcss": "latest",
    "vinxi": "^0.5.3",
    "xstate": "^5.19.2",
    "zod": "^4.1.12"
  },
  "engines": {
    "node": ">=20"
  },
  "packageManager": "pnpm@9.15.2"
}
